page_data = [
<% @contents.each do |content| -%>
  '<%= radio_button_tag "target_contents", content[:type], (params[:target_contents] == content[:type] || content[:type].empty? ) && params[:target_aid] == 'skip' -%>',
  '<%= label_tag("target_contents_#{content[:type]}", icon_tag(content[:icon], :margin => true) + content[:name]) -%>',
<% end -%>
].join(' ');

document.write(page_data);
